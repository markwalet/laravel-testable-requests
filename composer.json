{
    "name": "markwalet/laravel-testable-requests",
    "description": "A Laravel package to make request testing easier",
    "type": "library",
    "keywords": [
        "phpunit",
        "laravel",
        "testing"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/markwalet/laravel-testable-requests/issues"
    },
    "authors": [
        {
            "name": "Mark Walet",
            "email": "mark.walet@gmail.com",
            "homepage": "https://markwalet.me",
            "role": "Owner"
        }
    ],
    "autoload": {
        "psr-4": {
            "MarkWalet\\TestableRequests\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
